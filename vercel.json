{
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/:path(.*)",
      "has": [{ "type": "host", "value": "gatekeeper-nine.vercel.app" }],
      "destination": "https://zule.mazzizax.net/:path",
      "permanent": false
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
